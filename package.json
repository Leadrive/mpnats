    "copy": "cp ./dist/wx-nats.js ./examples/mp-fragment/utils/wx-nats.js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitee.com:xharry/nats.git"
  },
  "ava": {
		"babel": {
			"testOptions": {
				"babelrc": false,
				"configFile": false
			}
		}
	},
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "ava": "^1.0.0-rc.2",
    "cp-file": "^6.0.0",
    "gulp": "^3.9.1",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0"
  }
}
